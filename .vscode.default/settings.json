{
    "files.associations": {
        "iosfwd": "cpp",
        "filesystem": "cpp"
    },
    "[cpp]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true
    },
    "editor.insertSpaces": true,
    "cmake.debugConfig": {
        "args": [
            "${workspaceFolder}/testing/working/input.k",
            "${workspaceFolder}/testing/working/input.ll"
        ]
    },
    "C_Cpp.intelliSenseEngineFallback": "Enabled"
}